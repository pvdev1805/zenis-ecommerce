.startbar.d-print-none
  // start brand
  .brand
    a.logo(href=`/${pathAdmin}/dashboard`)
      span
        img.logo-sm(
          alt="logo-small" 
          src="/admin/assets/images/logo-sm.png"
          )
      span
        img.logo-lg.logo-light(
          alt="logo-large" 
          src="/admin/assets/images/logo-light.png"
          )
        img.logo-lg.logo-dark(
          alt="logo-large" 
          src="/admin/assets/images/logo-dark.png"
          )
  // end brand
  // start startbar-menu
  .startbar-menu
    #startbarCollapse.startbar-collapse(data-simplebar="")
      .d-flex.align-items-start.flex-column.w-100
        // Navigation
        ul.navbar-nav.mb-auto.w-100
          li.nav-item
            a.nav-link( 
              href=`/${pathAdmin}/dashboard`
              )
              i.iconoir-home-simple.menu-icon
              span Dashboards
            
          // end nav-item
          li.nav-item
            a.nav-link(
              aria-controls="sidebarArticle" 
              aria-expanded="false" 
              role="button" 
              data-bs-toggle="collapse" 
              href="#sidebarArticle"
              )
              i.iconoir-journal-page.menu-icon
              span Articles
            #sidebarArticle.collapse
              ul.nav.flex-column
                li.nav-item
                  a.nav-link(href=`/${pathAdmin}/article/create`) Create Article
                // end nav-item
                li.nav-item
                  a.nav-link(href=`/${pathAdmin}/article/list`) Article List
                // end nav-item
                li.nav-item
                  a.nav-link(href=`/${pathAdmin}/article/category`) Article Category
                // end nav-item
              // end nav
            // end startbarEmailTemplates
          // end nav-item
    // end startbar-collapse
  // end startbar-menu
// end startbar
.startbar-overlay.d-print-none